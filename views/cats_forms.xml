<?xml version="1.0" encoding="UTF-8"?>
<odoo>

  <record model="ir.ui.view" id="contributors_contrib_form">
    <field name="name">contributors.contrib.form</field>
    <field name="model">contributors.contrib</field>
    <field name="priority" eval="1"/>
    <field name="arch" type="xml">
      <form>
        <sheet>
          <group>
            <field name="name"/>
            <field name="nim"/>
            <field name="tarif"/>
            <field name="contact"/>
            <field name="achievement"/>
            <field name="contribution"/>
          </group>
        </sheet>
      </form>
    </field>
  </record>  
</odoo>